#include <stdio.h>
int main(){

  int a,b;
  int quotient;
  int modulus;
  printf ("Input a= ");
  scanf ("%d", &a);
  printf ("Input b= ");
  scanf ("%d", &b);

  if(b!=0){
    quotient=a/b;
    modulus = a%b;
  } else {
    printf ("Invalid!!!");
  }
  printf ("Quotient of %d and %d = %d\n", a, b, quotient);
  printf ("Modulus of %d and %d = %d\n", a, b, modulus);
  return 0;
}
