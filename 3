#include <stdio.h>
int main(){
char firstname[20], lastname[20];
int year;
printf ("Input your firstname: ");
scanf ("%s", &firstname);
printf ("\nInput your lastname: ");
scanf ("%s", &lastname);
printf ("\nInput your year of birth: ");
scanf ("%d", &year);
printf ("\n%s %s %d", firstname, lastname, year);
return 0;
}
